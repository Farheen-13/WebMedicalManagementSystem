<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
    <display-name>Medical Management System</display-name>

    <!-- Admin Login Servlet -->
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>admin.AdminLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>

    <!-- Doctor Login Servlet -->
    <servlet>
        <servlet-name>DoctorLogin</servlet-name>
        <servlet-class>doctor.DoctorLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DoctorLogin</servlet-name>
        <url-pattern>/doctorlogin</url-pattern>
    </servlet-mapping>
</web-app>
